{
    "name": "seogenie",
    "version": "1.0.0",
    "description": "SEOgenie",
    "author": "@seo_genie_team",
    "private": true,
    "scripts": {
        "dev": "sls offline start --skipCacheInvalidation",
        "dev:api": "./node_modules/.bin/swagger project edit",
