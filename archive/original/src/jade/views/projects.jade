.pull-right
    button(type="button").btn.btn-default.btn-sm Create project
h3 Projects

.row
    .col-lg-4.col-md-6
        .panel.b
            .panel-heading
                .pull-right
                    .label.label-info started
                h4.m0 Project #1
                small.text-muted Sed amet lectus id.
            .panel-body
                .table-grid.table-grid-align-middle
                    .col
                        .visible-xs.visible-sm.text-bold.text-muted.text-right 22%
                        div(uib-tooltip="Health"): uib-progressbar.progress-xs.m0(value='22', type='warning') 22%
                    .col.wd-xxs.text-right.hidden-xs.hidden-sm: .text-bold.text-muted 22%
            table.table: tbody
                tr
                    td: strong Start date
                    td 01/01/2016
                tr
                    td: strong  Members
                    td
                        a.inline(href=""): img.img-circle.thumb24(src="app/img/user/02.jpg", alt="project member")
                        a.inline(href=""): img.img-circle.thumb24(src="app/img/user/04.jpg", alt="project member")
                        a.inline(href=""): img.img-circle.thumb24(src="app/img/user/05.jpg", alt="project member")
                        a.inline(href=""): img.img-circle.thumb24(src="app/img/user/06.jpg", alt="project member")
                        a.inline(href=""): strong +5
                tr
                    td: strong  Leader
                    td
                        a(href="", uib-tooltip="Team leader"): img.img-circle.thumb24(src="app/img/user/03.jpg", alt="project member")
                tr
                    td: strong Metrics
                    td
                        .sparkline.inline.mr(sparkline="", values="20,80", data-type='pie', data-height="24", data-slice-colors="{{[colorByName('gray-lighter'), colorByName('info')]}}")
                        .sparkline.inline.mr(sparkline="", values="60,40", data-type='pie', data-height="24", data-slice-colors="{{[colorByName('gray-lighter'), colorByName('success')]}}")
                        .sparkline.inline(sparkline="", values="90,10", data-type='pie', data-height="24", data-slice-colors="{{[colorByName('gray-lighter'), colorByName('warning')]}}")
            .panel-footer.text-center
                button(type="button").btn.btn-default Manage project
