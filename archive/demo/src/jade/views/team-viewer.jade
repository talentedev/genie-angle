    em.fa.fa-warning.fa-lg.text-warning

h3 Team viewer

.row
    .col-lg-4.col-md-6
        // Team Panel
        .panel.panel-default
            .panel-heading
                .pull-right
                    .label.label-success free
                .panel-title Team #1
            .panel-body.bt
                a.inline(href="", uib-tooltip="Team leader"): img.img-circle.thumb48(src="app/img/user/03.jpg", alt="project member")
                a.inline(href=""): img.img-circle.thumb24(src="app/img/user/02.jpg", alt="project member")
                a.inline(href=""): img.img-circle.thumb24(src="app/img/user/04.jpg", alt="project member")
                a.inline(href=""): img.img-circle.thumb24(src="app/img/user/05.jpg", alt="project member")
                a.inline(href=""): img.img-circle.thumb24(src="app/img/user/06.jpg", alt="project member")
                a.inline(href=""): img.img-circle.thumb24(src="app/img/user/07.jpg", alt="project member")
            .panel-body.bb.text-center
                div(uib-tooltip="Progress of asigned tasks")
                    uib-progressbar.progress-xs.m0(value='50', type='info') 50%
            .panel-body.bb
                .row.text-center
                    .col-xs-4
                        p.text-bold BUDGET
                        .sparkline.inline.mr(sparkline="", values="60,40", data-type='pie', data-height="24", data-slice-colors="{{[colorByName('gray-lighter'), colorByName('warning')]}}")
                    .col-xs-4
                        p.text-bold TASKS
                        .h3.m0 26
                    .col-xs-4
                        p.text-bold AVAILABITY
                        .sparkline.inline.mr(sparkline="", values="70,30", data-type='pie', data-height="24", data-slice-colors="{{[colorByName('gray-lighter'), colorByName('warning')]}}")
            .panel-body
                ul.list-inline.m0
                    li: .badge.bg-gray angularjs
                    li: .badge.bg-gray jquery
                    li: .badge.bg-gray gulp
                    li: .badge.bg-gray git
                    li: .badge.p-sm.bg-gray ios
            .panel-footer.text-center
                a(href="").btn.btn-default.btn-oval Manage Team
        // end Team Panel
