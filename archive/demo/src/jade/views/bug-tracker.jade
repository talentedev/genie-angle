                    .col-xs-3.col-md-6.col-lg-3
                        .inline.mv
                            .sparkline(sparkline="", values="20,80", data-type='pie', data-height="50", data-slice-colors="{{[colorByName('gray-lighter'), colorByName('warning')]}}")
                        p.mt-lg Hours
                    .col-xs-3.col-md-6.col-lg-3
                        .inline.mv
                            .sparkline(sparkline="", values="30,70", data-type='pie', data-height="50", data-slice-colors="{{[colorByName('gray-lighter'), colorByName('danger')]}}")
                        p.mt-lg Assigned
            table.table.bb: tbody
