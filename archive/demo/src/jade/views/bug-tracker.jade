h3 Bug tracker

.row
    .col-md-4
        // Aside panel
        .panel.b
            .panel-body.bb
                p Overvall progress
                .table-grid.table-grid-align-middle.mv
                    .col: uib-progressbar.progress-xs.m0(value='48', type='info') 48%
                    .hidden-xs.col.wd-xxs.text-right: .text-bold.text-muted 48%
            .panel-body
                p Metrics
                .row.text-center
                    .col-xs-3.col-md-6.col-lg-3
                        .inline.mv
                            .sparkline(sparkline="", values="20,80", data-type='pie', data-height="50", data-slice-colors="{{[colorByName('gray-lighter'), colorByName('info')]}}")
                        p.mt-lg Issues
                    .col-xs-3.col-md-6.col-lg-3
                        .inline.mv
                            .sparkline(sparkline="", values="60,40", data-type='pie', data-height="50", data-slice-colors="{{[colorByName('gray-lighter'), colorByName('success')]}}")
